# Primary Telegram Bot
TELEGRAM_BOT_TOKEN=your_bot_token_here

# Worker Bots (comma-separated, up to 20)
# Each bot can handle ~20 messages/min, so 20 bots = 400 messages/min
WORKER_BOT_TOKENS=token1,token2,token3,token4,token5,token6,token7,token8,token9,token10,token11,token12,token13,token14,token15,token16,token17,token18,token19,token20

# Log Channel
LOG_CHANNEL_ID=-1003065136240

# Spotify API
SPOTIFY_CLIENT_ID=your_id
SPOTIFY_CLIENT_SECRET=your_secret

# Bot Settings
WORKER_POOL_SIZE=100
MAX_CONCURRENT_DOWNLOADS=50
IMAGE_DOWNLOAD_TIMEOUT_SEC=15
PROCESS_TIMEOUT_MIN=30

# Telegram Limits
MAX_ALBUM_SIZE=10
MAX_FILE_SIZE_MB=20
MAX_MESSAGES_PER_SECOND=15

# Inline Mode
INLINE_CACHE_TIME=300
MAX_INLINE_RESULTS=50

# Debug
DEBUG=false
LOG_LEVEL=info

# Auto-Playlist Feature
ENABLE_AUTO_PLAYLIST=true
AUTO_PLAYLIST_ID=2RlUagSNQ8eJ3P9hJQ4nxX
SPOTIFY_REFRESH_TOKEN=your_refresh_token_here or use spotify_token_cache.json